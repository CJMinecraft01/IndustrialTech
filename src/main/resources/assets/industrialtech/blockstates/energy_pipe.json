{
    "multipart": [
        {   "apply": { "model": "industrialtech:energy_pipe_center" }},
        {   "when": { "north": "pipe" },
            "apply": { "model": "industrialtech:energy_pipe_connected_pipe" }
        },
        {   "when": { "east": "pipe" },
            "apply": { "model": "industrialtech:energy_pipe_connected_pipe", "y": 90 }
        },
        {   "when": { "south": "pipe" },
            "apply": { "model": "industrialtech:energy_pipe_connected_pipe", "y": 180 }
        },
        {   "when": { "west": "pipe" },
            "apply": { "model": "industrialtech:energy_pipe_connected_pipe", "y": 270 }
        },
        {   "when": { "up": "pipe" },
            "apply": { "model": "industrialtech:energy_pipe_connected_pipe", "x": 270 }
        },
        {   "when": { "down": "pipe" },
            "apply": { "model": "industrialtech:energy_pipe_connected_pipe", "x": 90 }
        },
		{   "when": { "north": "block" },
            "apply": { "model": "industrialtech:energy_pipe_connected_block" }
        },
        {   "when": { "east": "block" },
            "apply": { "model": "industrialtech:energy_pipe_connected_block", "y": 90 }
        },
        {   "when": { "south": "block" },
            "apply": { "model": "industrialtech:energy_pipe_connected_block", "y": 180 }
        },
        {   "when": { "west": "block" },
            "apply": { "model": "industrialtech:energy_pipe_connected_block", "y": 270 }
        },
        {   "when": { "up": "block" },
            "apply": { "model": "industrialtech:energy_pipe_connected_block", "x": 270 }
        },
        {   "when": { "down": "block" },
            "apply": { "model": "industrialtech:energy_pipe_connected_block", "x": 90 }
        }
    ]
}
